<?xml version="1.0" encoding="UTF-8"?>
<databaseChangeLog
        xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
        xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
        xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog
                      http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-4.31.xsd"
        objectQuotingStrategy="QUOTE_ONLY_RESERVED_WORDS">
    <changeSet id="1756814649370-3" author="yusssss">
        <dropForeignKeyConstraint baseTableName="job_applications" constraintName="fk_job_applications_on_job"/>
    </changeSet>
    <changeSet id="1756814649370-4" author="yusssss">
        <dropForeignKeyConstraint baseTableName="job_applications" constraintName="fk_job_applications_on_user"/>
    </changeSet>
    <changeSet id="1756814649370-1" author="yusssss">
        <addForeignKeyConstraint baseColumnNames="job_id" baseTableName="job_applications"
                                 constraintName="FK_JOB_APPLICATIONS_ON_JOB" referencedColumnNames="id"
                                 referencedTableName="jobs"/>
    </changeSet>
    <changeSet id="1756814649370-2" author="yusssss">
        <addForeignKeyConstraint baseColumnNames="user_id" baseTableName="job_applications"
                                 constraintName="FK_JOB_APPLICATIONS_ON_USER" referencedColumnNames="id"
                                 referencedTableName="users"/>
    </changeSet>

</databaseChangeLog>